        "pdfjs-dist": "2.5.207"
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 75,
        "statements": 75,
        "functions": 75,
        "branches": 75,
        "include": [
            "server/**/*.js"
        ],
        "exclude": [
            "src/**/*.spec.js",
            "server/test/**/*.js",
            "server/node_modules/**/*",
            "server/datasources.*",
            "server/middleware.*",
            "server/utils/constants.utils.js",
            "server/config.*",
            "server/server.js"
        ],
        "reporter": [
            "lcov",
            "text-summary"
        ],
        "require": [],
        "extension": [],
        "cache": false,
        "all": true
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "heroku-run-build-script": true
}